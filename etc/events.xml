<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Event/etc/events.xsd">
    <event name="customer_save_after">
        <observer name="customer_save_after_cyclr_webhook_observer" instance="Cyclr\Webhook\Model\Observer\Customer\Save" />
    </event>
    <event name="customer_delete_after">
        <observer name="customer_delete_after_cyclr_webhook_observer" instance="Cyclr\Webhook\Model\Observer\Customer\Delete" />
    </event>
    <event name="newsletter_subscriber_save_after">
        <observer name="subscriber_save_after_cyclr_webhook_observer" instance="Cyclr\Webhook\Model\Observer\Subscriber\Save" />
    </event>
    <event name="catalog_product_save_after">
        <observer name="product_save_after_cyclr_webhook_observer" instance="Cyclr\Webhook\Model\Observer\Product\Save" />
    </event>
    <event name="catalog_product_delete_after">
        <observer name="product_delete_after_cyclr_webhook_observer" instance="Cyclr\Webhook\Model\Observer\Product\Delete" />
    </event>
    <event name="sales_order_save_after">
        <observer name="sales_order_save_after_cyclr_webhook_observer" instance="Cyclr\Webhook\Model\Observer\Order\Save" />
    </event>
</config>
